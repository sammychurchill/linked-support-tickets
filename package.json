{
  "name": "link-to-external-ticket",
  "version": "0.0.1",
  "description": "Process for sending a Zendesk ticket to a third-party and keeping a record in both tickets",
  "scripts": {
    "clean": "rimraf dist/tmp",
    "build": "webpack --config webpack.prod.js -p",
    "build-dev": "webpack --config webpack.dev.js",
    "package": "npm-run-all clean && zat package --path ./dist",
    "build:package": "npm-run-all build package",
    "watch": "webpack --config webpack.dev.js",
    "serve": "zat server --path dist -c settings.json",
    "python": "python3 -m http.server --directory dist/assets",
    "bserve": "webpack --config webpack.dev.js && zat server --path dist -c settings.json",
    "test": "jest"
  },
  "author": "SC",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.6",
    "@babel/preset-react": "^7.7.4",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-preset-es2016": "^6.24.1",
    "css-loader": "^3.3.0",
    "eslint": "^6.7.2",
    "eslint-import-resolver-webpack": "^0.12.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-watch": "^6.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.0.1",
    "svg-inline-loader": "^0.8.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.6",
    "@zendeskgarden/react-buttons": "^8.15.1",
    "@zendeskgarden/react-dropdowns": "^8.15.1",
    "@zendeskgarden/react-forms": "^8.15.1",
    "@zendeskgarden/react-grid": "^8.15.1",
    "@zendeskgarden/react-loaders": "^8.15.1",
    "@zendeskgarden/react-theming": "^8.15.1",
    "@zendeskgarden/react-typography": "^8.15.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "node-sass": "^4.13.0",
    "promise-polyfill": "^8.1.3",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-use": "^15.3.2",
    "serialize-javascript": "^2.1.2",
    "styled-components": "^5.1.1"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss)$": "identity-obj-proxy"
    }
  }
}
